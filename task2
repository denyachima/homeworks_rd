class TelegramBot(Bot):
    def __init__(self, name):
        super().__init__(name)
        self.url = None
        self.chat_id = None
        
    def send_message(self, message):
        if self.url is None:
            print("Cannot send message. URL is not set.")
            return
        if self.chat_id is None:
            print("Cannot send message. Chat ID is not set.")
            return
        print(f"Sending message '{message}' to {self.url} and chat id {self.chat_id}")
    
    def set_url(self, url):
        self.url = url
        
    def set_chat_id(self, chat_id):
        self.chat_id = chat_id
