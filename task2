def fibonacci(n):
    if n <= 1:  # базовий випадок: якщо n дорівнює 0 або 1, то повертаємо n
        return n
    else:  # рекурсивний випадок: обчислюємо суму двох попередніх чисел в послідовності
        return fibonacci(n-1) + fibonacci(n-2)

n = int(input("Введіть номер числа у послідовності Фібоначчі: "))
fib = fibonacci(n)
print(f"Число у послідовності Фібоначчі з номером {n} дорівнює {fib}")
